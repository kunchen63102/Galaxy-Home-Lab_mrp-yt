{
  "version": "2",
  "templates": [
    {
      "category": [
        "Monitoring"
      ],
      "description": "Zabbix - Enterprise-class open source distributed monitoring solution.",
      "env": [
        {
          "label": "MYSQL_DATABASE",
          "name": "MYSQL_DATABASE",
          "set": "zabbix"
        },
        {
          "label": "MYSQL_USER",
          "name": "MYSQL_USER",
          "set": "zabbix"
        },
        {
          "label": "MYSQL_PASSWORD",
          "name": "MYSQL_PASSWORD",
          "set": "zabbix_password"
        },
        {
          "label": "MYSQL_ROOT_PASSWORD",
          "name": "MYSQL_ROOT_PASSWORD",
          "set": "root_password"
        },
        {
          "label": "DB_DATA",
          "name": "DB_DATA",
          "set": "/path/to/db/data"
        },
        {
          "label": "ZABBIX_SERVER_DATA",
          "name": "ZABBIX_SERVER_DATA",
          "set": "/path/to/zabbix/server/data"
        },
        {
          "label": "HOST_PORT",
          "name": "HOST_PORT",
          "set": "8080"
        }
      ],
      "image": "",
      "logo": "https://upload.wikimedia.org/wikipedia/commons/thumb/4/4e/Zabbix_logo.svg/1024px-Zabbix_logo.svg.png",
      "platform": "linux",
      "ports": [],
      "title": "Zabbix",
      "type": 3,
      "restart_policy": "unless-stopped",
      "volumes": [],
      "network": "zabbix-network",
      "services": [
        {
          "name": "zabbix-db",
          "image": "mysql:8.0",
          "environment": [
            "MYSQL_DATABASE=${MYSQL_DATABASE}",
            "MYSQL_USER=${MYSQL_USER}",
            "MYSQL_PASSWORD=${MYSQL_PASSWORD}",
            "MYSQL_ROOT_PASSWORD=${MYSQL_ROOT_PASSWORD}"
          ],
          "volumes": [
            "${DB_DATA}:/var/lib/mysql"
          ],
          "command": "--character-set-server=utf8mb4 --collation-server=utf8mb4_unicode_ci",
          "networks": ["zabbix-network"]
        },
        {
          "name": "zabbix-server",
          "image": "zabbix/zabbix-server-mysql:latest",
          "environment": [
            "DB_SERVER_HOST=zabbix-db",
            "MYSQL_DATABASE=${MYSQL_DATABASE}",
            "MYSQL_USER=${MYSQL_USER}",
            "MYSQL_PASSWORD=${MYSQL_PASSWORD}",
            "MYSQL_ROOT_PASSWORD=${MYSQL_ROOT_PASSWORD}"
          ],
          "depends_on": ["zabbix-db"],
          "ports": [
            "10051:10051"
          ],
          "volumes": [
            "${ZABBIX_SERVER_DATA}:/var/lib/zabbix"
          ],
          "networks": ["zabbix-network"]
        },
        {
          "name": "zabbix-web",
          "image": "zabbix/zabbix-web-nginx-mysql:latest",
          "environment": [
            "ZBX_SERVER_HOST=zabbix-server",
            "DB_SERVER_HOST=zabbix-db",
            "MYSQL_DATABASE=${MYSQL_DATABASE}",
            "MYSQL_USER=${MYSQL_USER}",
            "MYSQL_PASSWORD=${MYSQL_PASSWORD}",
            "MYSQL_ROOT_PASSWORD=${MYSQL_ROOT_PASSWORD}"
          ],
          "ports": [
            "${HOST_PORT}:8080"
          ],
          "depends_on": ["zabbix-server", "zabbix-db"],
          "networks": ["zabbix-network"]
        },
        {
          "name": "zabbix-agent",
          "image": "zabbix/zabbix-agent:latest",
          "environment": [
            "ZBX_SERVER_HOST=zabbix-server"
          ],
          "depends_on": ["zabbix-server"],
          "networks": ["zabbix-network"]
        }
      ]
    },
    {
      "category": [
        "Tools"
      ],
      "description": "IT-Tools - Useful tools for developer and people working in IT.",
      "env": [
        {
          "label": "PGID",
          "name": "PGID",
          "set": "1000"
        },
        {
          "label": "PUID",
          "name": "PUID",
          "set": "1000"
        }
      ],
      "image": "corentinth/it-tools:latest",
      "logo": "https://www.vhv.rs/dpng/d/574-5747321_docker-ref-icon-docker-django-hd-png-download.png",
      "platform": "linux",
      "ports": [
        "8085/tcp"
      ],
      "title": "IT-Tools",
      "type": 1
    },
    {
      "category": [
        "Tools"
      ],
      "description": "FileZilla - A fast and reliable FTP client.",
      "env": [
        {
          "label": "PUID",
          "name": "PUID",
          "set": "1000"
        },
        {
          "label": "PGID",
          "name": "PGID",
          "set": "1000"
        },
        {
          "label": "TZ",
          "name": "TZ",
          "set": "Europe/London"
        }
      ],
      "image": "lscr.io/linuxserver/filezilla:latest",
      "logo": "https://raw.githubusercontent.com/linuxserver/docker-templates/master/linuxserver.io/img/filezilla-icon.png",
      "platform": "linux",
      "ports": [
        "3300:3000/tcp",
        "3301:3001/tcp"
      ],
      "title": "FileZilla",
      "type": 1,
      "volumes": [
        {
          "container": "/config",
          "bind": "/path/to/config"
        },
        {
          "container": "/some_folder",
          "bind": "/some/folder/name"
        },
        {
          "container": "/some/folder2-optional",
          "bind": "/some/folder/name2-optional"
        }
      ],
      "restart_policy": "unless-stopped"
    }
  ]
}
