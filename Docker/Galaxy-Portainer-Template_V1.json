{
  "version": "2",
  "templates": [
      {
          "category": [
              "Tools"
          ],
          "description": "IT-Tools - Useful tools for developer and people working in IT.",
          "env": [
              {
                  "label": "PGID",
                  "name": "PGID",
                  "set": "1000"
              },
              {
                  "label": "PUID",
                  "name": "PUID",
                  "set": "1000"
              }
          ],
          "image": "corentinth/it-tools:latest",
          "logo": "https://www.vhv.rs/dpng/d/574-5747321_docker-ref-icon-docker-django-hd-png-download.png",
          "platform": "linux",
          "ports": [
              "8085/tcp"
          ],
          "title": "IT-Tools",
          "type": 1
      },
      {
          "category": [
              "Tools"
          ],
          "description": "FileZilla - A fast and reliable FTP client.",
          "env": [
              {
                  "label": "PUID",
                  "name": "PUID",
                  "set": "1000"
              },
              {
                  "label": "PGID",
                  "name": "PGID",
                  "set": "1000"
              },
              {
                  "label": "TZ",
                  "name": "TZ",
                  "set": "Europe/London"
              }
          ],
          "image": "lscr.io/linuxserver/filezilla:latest",
          "logo": "https://raw.githubusercontent.com/linuxserver/docker-templates/master/linuxserver.io/img/filezilla-icon.png",
          "platform": "linux",
          "ports": [
              "3300:3000/tcp",
              "3301:3001/tcp"
          ],
          "title": "FileZilla",
          "type": 1,
          "volumes": [
              {
                  "container": "/config",
                  "bind": "/path/to/config"
              },
              {
                  "container": "/some_folder",
                  "bind": "/some/folder/name"
              },
              {
                  "container": "/some/folder2-optional",
                  "bind": "/some/folder/name2-optional"
              }
          ],
          "restart_policy": "unless-stopped"
      }
  ]
}
